contact_instructions=During hours of service. Off on weekends.
contact_role=pointOfContact

[server]
url=http://localhost:{{ http_port }}/wps
outputurl=http://localhost:{{ http_port }}/outputs/{{ program_name }}/
outputpath=/var/lib/pywps/outputs/{{ program_name }}
workdir=/var/lib/pywps/tmp/{{ program_name }}
maxsingleinputsize=30mb
maxrequestsize=30mb
maxprocesses=30
parallelprocesses=10

[logging]
level=DEBUG
file=/var/log/pywps/{{ program_name }}.log
database=sqlite://///var/lib/pywps/db/{{ program_name }}_log.db
format=%(asctime)s] [%(levelname)s] %(message)s
